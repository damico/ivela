<ehcache xsi:noNamespaceSchemaLocation="ehcache.xsd" updateCheck="true" monitoring="autodetect">

<diskStore path="java.io.tmpdir"/>

<defaultCache maxElementsInMemory="1000" eternal="true" overflowToDisk="false" memoryStoreEvictionPolicy="LRU"/>

<cache name="authenticationCache" maxElementsInMemory="20" maxElementsOnDisk="0" eternal="true" overflowToDisk="false" memoryStoreEvictionPolicy="LFU" />

<cache name="courseCache" maxElementsInMemory="300" maxElementsOnDisk="0" eternal="true" overflowToDisk="false" memoryStoreEvictionPolicy="LFU" />

<cache name="gradeCache" maxElementsInMemory="700" maxElementsOnDisk="0" eternal="true" overflowToDisk="false" memoryStoreEvictionPolicy="LFU" />

<cache name="disciplineCache" maxElementsInMemory="1000" maxElementsOnDisk="0" eternal="true" overflowToDisk="false" memoryStoreEvictionPolicy="LRU" />

<cache name="systemUserCache" maxElementsInMemory="5000" maxElementsOnDisk="0" eternal="true" overflowToDisk="false" memoryStoreEvictionPolicy="LRU" />

<cache name="functionalityCache" maxElementsInMemory="5000" maxElementsOnDisk="0" eternal="true" overflowToDisk="false" memoryStoreEvictionPolicy="LRU" />

</ehcache>