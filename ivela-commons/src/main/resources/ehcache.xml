<ehcache xsi:noNamespaceSchemaLocation="ehcache.xsd" updateCheck="true" monitoring="autodetect">

<diskStore path="java.io.tmpdir"/>

<defaultCache maxElementsInMemory="1000" eternal="true" overflowToDisk="false" memoryStoreEvictionPolicy="LRU"/>

<cache name="authenticationCache" maxElementsInMemory="20" eternal="true" overflowToDisk="false" memoryStoreEvictionPolicy="LFU" />

<cache name="courseCache" maxElementsInMemory="300" eternal="true" overflowToDisk="false" memoryStoreEvictionPolicy="LFU" />

<cache name="unitContentCache" maxElementsInMemory="1000" eternal="true" overflowToDisk="false" memoryStoreEvictionPolicy="LFU" />

<cache name="unitCache" maxElementsInMemory="700" eternal="true" overflowToDisk="false" memoryStoreEvictionPolicy="LFU" />

<cache name="forumCache" maxElementsInMemory="500" eternal="true" overflowToDisk="false" memoryStoreEvictionPolicy="LFU" />

<cache name="gradeCache" maxElementsInMemory="300" eternal="true" overflowToDisk="false" memoryStoreEvictionPolicy="LFU" />

<cache name="profileCache" maxElementsInMemory="700" eternal="true" overflowToDisk="false" memoryStoreEvictionPolicy="LFU" />

<cache name="enrollmentCache" maxElementsInMemory="5000" eternal="true" overflowToDisk="false" memoryStoreEvictionPolicy="LFU" />

<cache name="disciplineCache" maxElementsInMemory="1000"  eternal="true" overflowToDisk="false" memoryStoreEvictionPolicy="LRU" />

<cache name="systemUserCache" maxElementsInMemory="5000"  eternal="true" overflowToDisk="false" memoryStoreEvictionPolicy="LRU" />

<cache name="functionalityCache" maxElementsInMemory="5000" eternal="true" overflowToDisk="false" memoryStoreEvictionPolicy="LRU" />

<cache name="contentInfoCache" maxElementsInMemory="2000" eternal="true" overflowToDisk="false" memoryStoreEvictionPolicy="LRU" />

<cache name="renderTextServletCache" maxElementsInMemory="4000" eternal="true" overflowToDisk="false" memoryStoreEvictionPolicy="LFU" />

<cache name="renderBinaryServletCache" maxElementsInMemory="4000" eternal="true" overflowToDisk="false" memoryStoreEvictionPolicy="LFU" />

<cache name="org.hibernate.cache.StandardQueryCache"
    maxElementsInMemory="20" eternal="false" timeToLiveSeconds="240"
    overflowToDisk="false" />

<cache name="org.hibernate.cache.UpdateTimestampsCache"
    maxElementsInMemory="5000" eternal="true" overflowToDisk="false" />
    
</ehcache>